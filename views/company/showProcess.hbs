<h1>{{process.title}}</h1>
<h5>{{process.jobRole}} ({{process.hierarchyOfRole}})</h5>
{{#if company}}
  <p>Link for candidates to enroll: <br><a href="{{link}}">{{link}}</a></p>
{{/if}}

<h3>Prerequisites</h3>
<ul>
  {{#each catQuestRel}}
    <li><b>{{this.[0]}}</b> - ({{this.1.length}} questions)</li>
  {{/each}}
</ul>
{{#if company}}
 {{#if users}}
    <h3>Candidates:</h3>
  <ul>
  {{#each users}}
    {{#if personal.name}}
    <li><a href="/user/profile/{{_id}}">{{personal.name}} {{personal.lastName}}</a></li>
    {{else}}
    <li><a href="/user/profile/{{_id}}">{{personal.email}}</a></li>
    {{/if}}
  {{/each}}
  </ul>
 {{else}}
  <h3>No candidates yet!</h3>
 {{/if}}

<a href="/company/processes">Back</a>
{{else}}
  <a href="/user/process/{{process._id}}/test">Take the test!</a>
{{/if}}


